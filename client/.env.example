# Backend API Configuration
# For development
REACT_APP_API_BASE_URL=http://localhost:5000

# For production - set this to your deployed backend URL
# REACT_APP_API_BASE_URL=https://your-backend-url.com

# Supabase Configuration
REACT_APP_SUPABASE_URL=your_supabase_url_here
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# OpenStreetMap with Leaflet.js - No API key needed! 
# Just remove the Google Maps variables below

# REACT_APP_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE (Not needed anymore)

# For server-side geocoding (optional)
# NOMINATIM_USER_AGENT=your-app-name
